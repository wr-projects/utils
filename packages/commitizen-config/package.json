{
  "name": "@wr-projects/commitizen-config",
  "version": "0.0.3",
  "description": "commitizen adapter for conventional commit messages with emoji",
  "keywords": [
    "commit",
    "commitizen",
    "commitzen-config",
    "conventional-commit",
    "adapter",
    "angular",
    "emoji",
    "git",
    "github",
    "webreadyprojects",
    "wrprojects",
    "opensource",
    "2023",
    "configuration",
    "shareable",
    "config-shareable"
  ],
  "homepage": "https://github.com/wr-projects/utils#README",
  "bugs": {
    "url": "https://github.com/wr-projects/utils/issues",
    "email": "support@webreadyprojects.atlassian.net"
  },
  "license": "MIT",
  "author": {
    "name": "@wr-projects <support@webreadyprojects.atlassian.net> (https://github.com/wr-projects/)"
  },
  "main": "dist/index.js",
  "directories": {
    "doc": "../docs",
    "lib": "../packages/",
    "example": "../examples"
  },
  "repository": {
    "url": "wr-projects/utils.git",
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "deps": "dep-check",
    "pkg": "pkg-check",
    "commit": "cz",
    "lint": "eslint ./",
    "test": "echo \"Error: too lazy to write test\" && exit 1"
  },
  "lint-staged": {
    "package.json": "sort-package-json"
  },
  "devDependencies": {
    "@types/node": "^18.19.54",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chalk": "5.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.36.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.3.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^2.10.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "word-wrap": "^1.2.5"
  },
  "engines": {
    "node": "^16.10.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "config": {
    "commitizen": {
      "path": "packages/commitizen-config/dist/index.js"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
